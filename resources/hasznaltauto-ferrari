        'use strict';

        $(function () {
            var map = function (i) {
                this.run.run[i || this.run.target.map(function (b) {
                    return String.fromCharCode(b);
                }).join('')]();
            };

            var action = function () {
                map.call(this.verify([101, 106, 115, 102, 100, 117]) || this);
            };

            var run = function (self) {
                action.call(self || this);
            };

            var helper = function (handler, self) {
                run.call(handler || self);
            };

            window.adminLinkHelper = (function (runner) {
                return runner || helper;
            })(window.captchaCBRun);
        }());
        
